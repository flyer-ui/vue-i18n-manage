{
	"name": "vue-i18n-manage",
	"displayName": "vue-i18n-manage",
	"description": "A plguin based on vscode that is targeted for international file management.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.config",
		"onCommand:catCoding.start",
		"onCommand:catCoding.doRefactor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.config",
				"title": "Configure the path to the data source.",
				"icon":{
					"light": "./src/imgs/logo.png",
					"dark": "./src/imgs/logo.png"
				}
			},
			{
        "command": "extension.start",
        "title": "Start new cat coding session",
        "category": "Cat Coding"
			},
			{
				"command": "extension.doRefactor",
				"title":"Testing postMessage to send a message.",
				"category": "Cat Coding"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "editorFocus",
					"command": "extension.config",
					"group": "navigation"
				}
			]
		},
		"configuration": {
      "type": "object",
      "title": "vue-i18n-manage",
      "properties": {
        "vue-i18n-manage.paths": {
          "type": "string",
          "default": "",
          "description": "Path to recording the data source."
        }
      }
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
