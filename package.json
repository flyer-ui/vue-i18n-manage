{
	"name": "vue-i18n-manage",
	"displayName": "Vue-i18n-manage",
	"description": "A plguin based on vscode that is targeted for international file management.",
	"publisher": "vue-i18n-manage",
	"version": "0.4.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.config",
		"onLanguage:vue",
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"keywords": [
		"vue",
		"vue-i18n",
		"i18n",
		"i18n manage",
		"i18n tool",
		"i18n helper"
	],
	"icon": "imgs/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/flyer-ui/vue-i18n-manage.git"
	},
	"homepage": "https://github.com/flyer-ui/vue-i18n-manage",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.config",
				"title": "Configure the path to the data source.",
				"icon": {
					"light": "imgs/logo.svg",
					"dark": "imgs/logo.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "editorFocus",
					"command": "extension.config",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "vue-i18n-manage",
			"properties": {
				"vue-i18n-manage.path": {
					"type": "string",
					"default": "",
					"description": "Path to recording the data source."
				},
				"vue-i18n-manage.primaryLanguage": {
					"type": "string",
					"default": "zh-CN",
					"description": "Configure the current primary language, which will be used by the development environment to determine if the same text exists in the internationalized data source for that language."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"deepmerge": "^3.2.0",
		"small-linked-list": "0.0.3"
	}
}
